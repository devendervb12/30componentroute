		controllerName="smax.batch30.A2.controller.ProductDetail"
		xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Product Detail">
		<content>
		 <VBox>
            <Text text="{ProductID}"/>	
            <Text text="{Name}"/>	
            <Text text="{Category}"/>	
